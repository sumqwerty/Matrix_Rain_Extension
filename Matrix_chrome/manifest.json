{
    "manifest_version": 2,
    "name": "Matrix",
    "version": "0.0.0.1",
    "icons": {
        "16" : "icon16.png",
        "32" : "icon32.png",
        "48" : "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["p5.min.js","sketch.js"]
        }
    ],
    "permissions": ["tabs" ],

	"browser_action": {
        "default_icon": "icon32.png",
        "default_popup": "pop.html",
		"default_title": "a popup will come here."
	}
}